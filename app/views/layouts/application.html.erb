<!DOCTYPE html>
<html>
<head>
  <title>Beertopia</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="navbar navbar-inverse navbar-static-top">
  <a class="navbar-brand" href='/'>THE BEERTOPIA PROJECT</a>
    <div class="container">
    <ul class='navbar-content'>
      <ul><a href="/">Home</ul>
      <ul><a href="/about">About</ul>
      <ul><a href="/">Homebrew</ul>
      <ul><a href="/breweries">Beerfinder</ul>
      <ul><a href="/categories">Shop</ul>
    </ul>
  </div>

    <ul class="nav navbar-nav navbar-right">
    </ul>
  </div>
  <div>
    <ul class="breadcrumb">
      <li><a href="/">Home</a><span class="divider"></span></li>
      <li><a href="/about">About</a><span class="divider"></span></li>
      <li><a href="/breweries">Beerfinder</a><span class="divider"></span></li>
      <li><a href="/items">Shop</a><span class="divider"></span></li>
    </ul>
  </div>
</div>

<%= yield %>

<%= javascript_include_tag '//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry' %>
<%= javascript_include_tag '//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' %>

</body>
</html>
